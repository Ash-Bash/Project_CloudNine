{
    "compilerOptions": {
        "incremental": true,
        "sourceMap": true,
        "noImplicitAny": true,
        "skipLibCheck": true,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "typeRoots": [
            "node_modules/@types"
        ],
   
    },
    "files": [
        "./ts/client.tsx"
    ],
    "include": [
        "./ts/**/*"
    ],
    "exclude": [
      "node_modules",
      "typings",
      "./../server",
      "./../server/**/*.ts"
    ]
}
